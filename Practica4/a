 if (number[j[N]] = 0) {
                            bigger[N] := false 
                        } else {
                            if ((number[j[N]]) > (number[N])) {
                                bigger[N] := false 
                            } else{
                                if (((number[j[N]]) = (number[N])) & ((j[N]) > N)) {
                                      bigger[N] := false 
                                }
                           }
                       }